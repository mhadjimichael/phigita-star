        {  /charcode exch def
           ( ) dup 0 charcode put insideplacechar
        } forall
    grestore
  end
} def

circtextdict begin

/findhalfangle
{
  stringwidth pop 2 div
  2 xradius mul pi mul div 360 mul
} def

/outsideplacechar
{
  circtextdict begin
    /char exch def
    /halfangle char findhalfangle def
    gsave
      halfangle neg rotate
      radius 0 translate
      -90 rotate
      char stringwidth pop 2 div neg 0 moveto
      char show
    grestore
    halfangle 2 mul neg rotate
  end
} def

/insideplacechar
{
  circtextdict begin
    /char exch def
    /halfangle char findhalfangle def
    gsave
      halfangle rotate
      radius 0 translate
      90 rotate
      char stringwidth pop 2 div neg 0 moveto
      char show
    grestore
    halfangle 2 mul rotate
  end
} def

/pi 3.1415923 def

% My own stuff follows

/mm { 72 mul 25.4 div } def

/Sector
  % stack:  radius1 radius2 ang1 ang2  Sector  --
{
  circtextdict begin
    /ang2 exch def
    /ang1 exch def
    gsave
      InteriorGray setgray
      newpath
        ang1 cos Radius2 mul ang1 sin Radius2 mul moveto
        0 0 Radius2 ang1 ang2 arc
        0 0 Radius1 ang2 ang1 arcn
        closepath
      fill
      BorderGray setgray
      newpath
        ang1 cos Radius2 mul ang1 sin Radius2 mul moveto
        0 0 Radius2 ang1 ang2 arc
        ang2 cos Radius1 mul ang2 sin Radius1 mul moveto
        0 0 Radius1 ang2 ang1 arcn
      stroke
    grestore
  end
} def

/SectorText
  % stack:  str ang1 ang2  Sector  --
{
  circtextdict begin
    /ang2 exch def
    /ang1 exch def
    gsave
      TextGray setgray
      Radius2 Radius1 sub 6 sub ang2 ang1 add 2 div Radius1 2 add
      outsidecircletext
    grestore
  end
} def

/Rings
  % stack: ULx ULy ig bg tg  Rings  --
{
  circtextdict begin
    /TextGray exch def
    /BorderGray exch def
    /InteriorGray exch def
    /ULy exch def
    /ULx exch def
    gsave
      ULx ULy translate
        90 360 Sector
        (BLUE)  90 180 SectorText
        (BLUE) 180 270 SectorText
        (BLUE) 270 360 SectorText
      HD VD neg translate
        90 360 Sector
        (YELLOW)   90 180 SectorText
        (YELLOW) 180 270 SectorText
        (YELLOW) 270 360 SectorText
      HD VD translate
        90 360 Sector
        (BLACK)  90 180 SectorText
        (BLACK) 180 270 SectorText
        (BLACK) 270 360 SectorText
      HD VD neg translate
        90 360 Sector
        (GREEN)  90 180 SectorText
        (GREEN) 180 270 SectorText
        (GREEN) 270 360 SectorText
      HD VD translate
        90 360 Sector
        (RED)  90 180 SectorText
        (RED) 180 270 SectorText
        (RED) 270 360 SectorText
        0 90 Sector
        (RED) 0 90 SectorText
      HD neg VD neg translate
        0 90 Sector
        (GREEN) 0 90 SectorText
      HD neg VD translate
        0 90 Sector
        (BLACK) 0 90 SectorText
      HD neg VD neg translate
        0 90 Sector
        (YELLOW) 0 90 SectorText
      HD neg VD translate
        0 90 Sector
        (BLUE) 0 90 SectorText
    grestore
  end
} def

