        } forall
    grestore
  end
} def

/insidecircletext
{
  circtextdict begin
    /radius exch def
    /centerangle exch def
    /ptsize exch def
    /str exch def
    /xradius radius ptsize 3 div sub def
    gsave
      centerangle str findhalfangle sub rotate
