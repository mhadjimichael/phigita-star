#!/bin/sh
psql -h turing -U postgres -t -c "select case when channel.train_p then channel.classification__tree_sk else null end, case when channel.train_p then channel.classification__edition_sk else null end, case when u.creation_date > current_timestamp - '1 week'::interval then u.classification__tree_sk else null end,  case when u.creation_date > current_timestamp - '1 week'::interval then u.classification__edition_sk else null end,  u.url_sha1, ts_vector from xo.xo__sw__agg__url channel inner join (xo.xo__sw__agg__url u1 inner join (select url,ts_vector from xo.xo__news_in_greek) u2 on (u1.url=u2.url)) u on (channel.url_sha1=u.channel_url_sha1) where channel.feed_p and not channel.buzz_p  and u.train_p and u.creation_date > current_timestamp-'1 week'::interval" -o /var/lib/naviserver/service-phgt-0/data/news/tmp/tc.psv newsdb
