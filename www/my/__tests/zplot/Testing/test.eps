% zdraw dictionary
/zdict 256 dict def
zdict begin
/m {moveto} bind def
/l {lineto} bind def
/mr {rmoveto} bind def
/lr {rlineto} bind def
/lshow { show } def
/rshow { dup stringwidth pop neg 0 mr show } def
/cshow { dup stringwidth pop -2 div 0 mr show } def
/cpx 150 def
/cpy 150 def
end
zdict begin
% testing this out
gsave
% small point stuff
newpath
10 10 m
0 1 lr
stroke
% text stuff
newpath
cpx cpy m
(Helvetica) findfont 10.0 scalefont setfont 
(newtext) lshow
currentpoint
/cpy exch def
/cpx exch def
stroke
cpx cpy m
2 0 mr
(aftertext) lshow
% new
50 100 m
(a b) show
currentpoint
pop
stroke
% 
grestore
% zdraw epilogue
end
showpage
